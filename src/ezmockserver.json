{
    "api": {
      "httpPort": 3050
    },
    "server": {
      "httpPort": 3002
    },
    "sessionsDirectory": "./sessions/",
    "defaultSession": {
      "name": "default"
    },
    "defaultMatchers": [
      {
        "name": "orders",
        "method": "^GET$",
        "url": "/api/orders*"
      },
      {
        "name": "order-api",
        "method": "^PUT$",
        "url": "/api/internal/tracking*"
      },
      {
        "name": "slack",
        "method": "^POST$",
        "url": "/slack*"
      },
      {
        "name": "order_by_query_params",
        "method": "^GET$",
        "url": "/api/orders\\?originCreatedAt*"
      },
      {
        "name": "order_by_id",
        "method": "^GET$",
        "url": "/api/orders/\\w+"
      }
    ]
  }
  